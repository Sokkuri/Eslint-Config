{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": [
            2,
            "double"
        ],
        "spaced-comment": 2,
        "no-unused-vars": 1,
        "semi": [
            "error",
            "always"
        ]
    }
}
