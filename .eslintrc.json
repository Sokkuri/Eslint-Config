{
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": [
            2,
            "double"
        ],
        "spaced-comment": 2,
        "no-unused-vars": 2,
        "@typescript-eslint/semi": [
            "error",
            "always"
        ]
    }
}
